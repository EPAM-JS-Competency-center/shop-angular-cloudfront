              "Bucket": {
                "Ref": "WebAppS3Bucket"
              },
              "PolicyDocument": {
                "Statement": [
                  {
                    "Sid": "AllowCloudFrontAccessIdentity",
                    "Effect": "Allow",
                    "Action": "s3:GetObject",
                    "Resource": {
                      "Fn::Join": [
                        "",
                        [
                          "arn:aws:s3:::",
                          {
                            "Ref": "WebAppS3Bucket"
                          },
                          "/*"
                        ]
                      ]
                    },
                    "Principal": {
                      "AWS": {
                        "Fn::Join": [
                          " ",
                          [
