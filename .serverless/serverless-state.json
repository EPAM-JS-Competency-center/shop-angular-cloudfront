                  "AllowedMethods": ["GET", "HEAD", "OPTIONS"],
                  "CachedMethods": ["GET", "HEAD", "OPTIONS"],
                  "ForwardedValues": {
                    "Headers": [
                      "Access-Control-Request-Headers",
                      "Access-Control-Request-Method",
                      "Origin",
                      "Authorization"
                    ],
                    "QueryString": false,
                    "Cookies": {
                      "Forward": "none"
                    }
                  },
                  "TargetOriginId": "S3StoreOrigin",
                  "ViewerProtocolPolicy": "redirect-to-https",
                  "Compress": true,
                  "DefaultTTL": 0
                },
